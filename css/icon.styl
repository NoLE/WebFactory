@font-face {
	font-family: 'icomoon';
	src:url('../fonts/icomoon.eot?cpnoea');
	src:url('../fonts/icomoon.eot?#iefixcpnoea') format('embedded-opentype'),
		url('../fonts/icomoon.woff?cpnoea') format('woff'),
		url('../fonts/icomoon.ttf?cpnoea') format('truetype'),
		url('../fonts/icomoon.svg?cpnoea#icomoon') format('svg');
	font-weight: normal;
	font-style: normal;
}

[class^="icon-"], [class*=" icon-"] {
	font-family: 'icomoon';
	speak: none;
	font-style: normal;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
	line-height: 1;

	/* Better Font Rendering =========== */
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
}

.icon-menu-responsive:before {
	content: "\f05e";
}
.icon-google:before {
	content: "\ea8a";
}
.icon-fbk:before {
	content: "\ea8e";
}
.icon-twitter3:before {
	content: "\ea93";
}
.icon-github4:before {
	content: "\eab4";
}
.icon-linkedin:before {
	content: "\eac8";
}

.icon-google:hover
	color #E14424
.icon-fbk:hover
	color #45619D
.icon-github4:hover
	color #602d4f
.icon-twitter3:hover
	color #55acee
.icon-linkedin:hover
	color #007bb6
